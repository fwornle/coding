@startuml
!include /Users/q284340/Agentic/coding/docs/puml/_standard-style.puml

package "UI Component Work Pattern Architecture" {
    
    abstract class PatternBase <<pattern>> {
        +name: String
        +category: String
        +description: String
        +significance: Integer
        +evidence: List<String>
        --
        +analyze(): AnalysisResult
        +getOccurrences(): Integer
        +getTrend(): String
    }
    
    class UiComponentWorkPattern <<implementation>> {
        -occurrences: Integer = 10
        -trend: String = "stable"
        -language: String = "Unknown"
        -usageNotes: List<String>
        --
        +processComponent(): ComponentResult
        +validatePattern(): Boolean
        +getImplementationDetails(): Implementation
        +trackOccurrences(): Integer
    }
    
    class PatternCatalog <<service>> {
        -patterns: List<Pattern>
        -totalPatterns: Integer = 1
        -avgSignificance: Double = 5.0
        -topPatterns: List<String>
        --
        +addPattern(pattern: Pattern): void
        +getPatternsByCategory(category: String): List<Pattern>
        +calculateSummary(): Summary
        +getTopPatterns(): List<String>
    }
    
    class Implementation <<value object>> {
        -language: String
        -usageNotes: List<String>
        --
        +getLanguage(): String
        +addUsageNote(note: String): void
        +validateImplementation(): Boolean
    }
    
    class PatternSummary <<data>> {
        -totalPatterns: Integer
        -byCategory: Map<String, Integer>
        -avgSignificance: Double
        -topPatterns: List<String>
        --
        +generateReport(): String
        +getCategoryDistribution(): Map<String, Integer>
    }
    
    enum PatternCategory <<enumeration>> {
        IMPLEMENTATION
        BEHAVIORAL
        STRUCTURAL
        CREATIONAL
    }
    
    class AnalysisData <<aggregate root>> {
        -patternCatalog: PatternCatalog
        -content: String
        -name: String
        --
        +performAnalysis(): AnalysisResult
        +exportData(): String
        +validateData(): Boolean
    }
    
    ' Relationships
    PatternBase <|-- UiComponentWorkPattern
    PatternCatalog *-- PatternBase : contains
    PatternCatalog *-- PatternSummary : generates
    UiComponentWorkPattern *-- Implementation : has
    UiComponentWorkPattern --> PatternCategory : categorized by
    AnalysisData *-- PatternCatalog : contains
    
    ' Notes
    note right of UiComponentWorkPattern
        10 occurrences detected
        Trend: stable
        Category: Implementation
        Significance: 5/10
    end note
    
    note top of PatternCatalog
        Manages collection of
        architectural patterns
        Total patterns: 1
    end note
}
@enduml