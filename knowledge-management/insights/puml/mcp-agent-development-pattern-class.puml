@startuml
!include /Users/q284340/Agentic/coding/docs/puml/_standard-style.puml

package "Pattern Catalog" {
  class McpAgentDevelopmentPattern {
    - name: String
    - category: String
    - description: String
    - significance: Integer
    + getEvidence(): List<String>
    + getRelatedComponents(): List<String>
    + getImplementation(): Implementation
  }

  class Implementation {
    - language: String
    - usageNotes: List<String>
  }

  McpAgentDevelopmentPattern --* Implementation
}

package "Summary" {
  class PatternSummary {
    - totalPatterns: Integer
    - byCategory: Map<String, Integer>
    - avgSignificance: Double
    - topPatterns: List<String>
  }

  PatternSummary ..> McpAgentDevelopmentPattern
}

@enduml