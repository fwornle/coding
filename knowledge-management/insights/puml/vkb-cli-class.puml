@startuml
!include /Users/q284340/Agentic/coding/docs/puml/_standard-style.puml

package "VkbCli" {
  class VkbCli {
    - graphDatabase: GraphDatabase
    - jsonExport: JSONExport
    + initialize()
    + storeData(data: String)
    + exportData()
  }

  class GraphDatabase {
    - graphology: Graphology
    - levelDB: LevelDB
    + storeData(data: String)
    + retrieveData(): String
  }

  class Graphology {
    + createGraph()
    + addNode(node: Node)
    + addEdge(edge: Edge)
  }

  class LevelDB {
    + storeData(data: String)
    + retrieveData(): String
  }

  class JSONExport {
    + exportData(data: String)
  }

  class GraphDatabaseService {
    - graphDatabaseAdapter: GraphDatabaseAdapter
    + processData(data: String)
  }

  class GraphDatabaseAdapter {
    - graphDatabase: GraphDatabase
    + storeData(data: String)
    + retrieveData(): String
  }

  VkbCli --* GraphDatabase
  VkbCli --* JSONExport
  GraphDatabase --* Graphology
  GraphDatabase --* LevelDB
  GraphDatabaseService --* GraphDatabaseAdapter
  GraphDatabaseAdapter --* GraphDatabase
}

package "Tools" {
  class VkbCommand {
    + visualizeGraph()
  }

  class MCPTools {
    + performKnowledgeOperations()
  }

  VkbCli --* VkbCommand
  VkbCli --* MCPTools
}

@enduml