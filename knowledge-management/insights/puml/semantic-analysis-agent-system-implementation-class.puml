@startuml
!include /Users/q284340/Agentic/coding/docs/puml/_standard-style.puml

package "SemanticAnalysisAgentSystemImplementation" {
  class GraphDatabase <<service>> {
    - graphology: Graphology
    - levelDB: LevelDB
    + storeData(data: String)
    + retrieveData(): String
  }

  class GraphDatabaseService <<service>> {
    - graphDatabase: GraphDatabase
    + manageDataStorage()
    + manageDataRetrieval()
  }

  class GraphKnowledgeExporter <<service>> {
    - graphDatabase: GraphDatabase
    + exportKnowledge()
  }

  class GraphDatabaseAdapter <<service>> {
    - graphDatabase: GraphDatabase
    + adaptData()
  }

  class CLI <<interface>> {
    + visualize()
    + knowledgeOperations()
  }

  class VKBCommand <<class>> {
    - cli: CLI
    + executeCommand()
  }

  class MCPTools <<class>> {
    - cli: CLI
    + performOperations()
  }

  class JSONExporter <<class>> {
    - graphKnowledgeExporter: GraphKnowledgeExporter
    + exportToJson()
  }

  GraphDatabaseService --* GraphDatabase
  GraphKnowledgeExporter --* GraphDatabase
  GraphDatabaseAdapter --* GraphDatabase
  VKBCommand --* CLI
  MCPTools --* CLI
  JSONExporter --* GraphKnowledgeExporter
  GraphDatabaseService ..> GraphKnowledgeExporter
  GraphDatabaseService ..> GraphDatabaseAdapter
}

@enduml