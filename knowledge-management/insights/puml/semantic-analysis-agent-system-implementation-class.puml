@startuml
!include /Users/q284340/Agentic/coding/docs/puml/_standard-style.puml

package "Knowledge Management" {
  class SemanticAnalysisAgentSystemImplementation <<MCPAgent>> {
    - graphDatabaseService: GraphDatabaseService
    - graphKnowledgeExporter: GraphKnowledgeExporter
    - persistenceAgent: PersistenceAgent
    + initialize()
    + start()
    + stop()
  }

  class GraphDatabaseService {
    - graph: Graph
    + createGraph()
    + storeData(data: String)
  }

  class GraphKnowledgeExporter {
    - graph: Graph
    + exportToJson()
  }

  class PersistenceAgent {
    - levelDB: LevelDB
    + persistData(data: String)
  }

  class Graph {
    - nodes: List<Node>
    - edges: List<Edge>
  }

  class Node {
    - id: String
    - properties: Map<String, String>
  }

  class Edge {
    - id: String
    - source: Node
    - target: Node
  }

  class LevelDB {
    - database: Database
    + storeData(data: String)
  }

  class Database {
    - filename: String
  }
}

package "Interfaces" {
  interface Graphology {
    + createGraph()
    + storeData(data: String)
  }

  interface LevelDBInterface {
    + storeData(data: String)
  }
}

SemanticAnalysisAgentSystemImplementation --* GraphDatabaseService
SemanticAnalysisAgentSystemImplementation --* GraphKnowledgeExporter
SemanticAnalysisAgentSystemImplementation --* PersistenceAgent
GraphDatabaseService --* Graph
GraphKnowledgeExporter --* Graph
PersistenceAgent --* LevelDB
LevelDB --* Database
GraphDatabaseService ..> Graphology
LevelDB ..> LevelDBInterface

@enduml