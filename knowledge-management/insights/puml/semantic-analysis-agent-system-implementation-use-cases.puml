@startuml
!include /Users/q284340/Agentic/coding/docs/puml/_standard-style.puml

actor Developer as "Developer"
actor SystemAdmin as "System Admin"

usecase "Manage Knowledge Graph" as (Manage Knowledge Graph)
usecase "Export Knowledge Graph" as (Export Knowledge Graph)
usecase "Perform Semantic Analysis" as (Perform Semantic Analysis)

Developer -- (Manage Knowledge Graph)
SystemAdmin -- (Export Knowledge Graph)
Developer -- (Perform Semantic Analysis)

package "SemanticAnalysisAgentSystemImplementation" {
  (Manage Knowledge Graph)
  (Export Knowledge Graph)
  (Perform Semantic Analysis)
}

usecase "Use Command-Line Interface" as (Use Command-Line Interface)
usecase "View Knowledge Export" as (View Knowledge Export)

Developer -- (Use Command-Line Interface)
SystemAdmin -- (View Knowledge Export)

(Use Command-Line Interface) ..> (Manage Knowledge Graph) : includes
(Use Command-Line Interface) ..> (Export Knowledge Graph) : includes
(View Knowledge Export) ..> (Export Knowledge Graph) : includes

package "Graph Database Service" {
  (Manage Knowledge Graph)
}

package "Graph Knowledge Exporter" {
  (Export Knowledge Graph)
}

package "Persistence Agent" {
  (Perform Semantic Analysis)
}

@enduml