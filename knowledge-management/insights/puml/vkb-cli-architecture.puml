@startuml
!include /Users/q284340/Agentic/coding/docs/puml/_standard-style.puml

package "VkbCli" {
  component "GraphDatabaseService" as "GraphDatabaseService [Core]" <<core>>
  component "GraphDatabaseAdapter" as "GraphDatabaseAdapter [Adapter]" <<api>>
  component "Graphology" as "Graphology [Storage]" <<storage>>
  component "LevelDB" as "LevelDB [Storage]" <<storage>>
  component "JSON Export" as "JSON Export [Storage]" <<storage>>
  component "vkb command" as "vkb command [Agent]" <<agent>>
  component "MCP tools" as "MCP tools [Agent]" <<agent>>
  
  GraphDatabaseService --* GraphDatabaseAdapter
  GraphDatabaseAdapter --* Graphology
  GraphDatabaseAdapter --* LevelDB
  Graphology --* LevelDB
  LevelDB --* JSON Export
  GraphDatabaseService --* vkb command
  GraphDatabaseService --* MCP tools
  vkb command --* GraphDatabaseService
  MCP tools --* GraphDatabaseService
  
  note as AutoNote1
  VkbCli: Dual-storage architecture with Graphology + LevelDB for primary storage and auto-synchronized JSON exports
  end note
}

@enduml