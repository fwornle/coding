{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "browser-access": {
      "command": "node",
      "args": ["{{CLAUDE_PROJECT_PATH}}/browser-access/dist/index.js"],
      "env": {
        "LOCAL_CDP_URL": "{{LOCAL_CDP_URL}}",
        "ANTHROPIC_API_KEY": "{{ANTHROPIC_API_KEY}}"
      }
    },
    "claude-logger": {
      "command": "node",
      "args": ["{{CLAUDE_PROJECT_PATH}}/claude-logger-mcp/dist/index.js"],
      "env": {
        "PROJECT_PATH": "{{CLAUDE_PROJECT_PATH}}"
      }
    }
  }
}