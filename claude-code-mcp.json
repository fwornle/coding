{
  "mcpServers": {
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "browser-access": {
      "command": "node",
      "args": ["{{CLAUDE_PROJECT_PATH}}/integrations/browser-access/dist/index.js"],
      "env": {
        "LOCAL_CDP_URL": "{{LOCAL_CDP_URL}}",
        "ANTHROPIC_API_KEY": "{{ANTHROPIC_API_KEY}}"
      }
    },
    "semantic-analysis": {
      "command": "node",
      "args": ["{{CODING_TOOLS_PATH}}/semantic-analysis-system/mcp-server/index.js"],
      "env": {
        "ANTHROPIC_API_KEY": "{{ANTHROPIC_API_KEY}}",
        "OPENAI_API_KEY": "{{OPENAI_API_KEY}}",
        "CODING_TOOLS_PATH": "{{CODING_TOOLS_PATH}}",
        "CODING_KB_PATH": "{{CODING_TOOLS_PATH}}/shared-memory.json",
        "NODE_ENV": "production",
        "LOG_LEVEL": "info"
      }
    }
  }
}